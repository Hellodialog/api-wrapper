{
    "name": "hellodialog/apiwrapper",
    "description": "HelloDialog e-mail marketing API handler",
    "keywords": [
        "hellodialog",
        "email-marketing",
        "api"
    ],
    "homepage": "https://github.com/Hellodialog/api-wrapper",
    "license": "MIT",
    "version": "v1.0.1",
    "authors": [
        {
            "name": "Bart Ros",
            "email": "bart@hellodialog.com",
            "homepage": "https://hellodialog.com",
            "role": "Developer"
        },
        {
            "name": "hellodialog",
            "email": "admin@hellodialog.com",
            "homepage": "https://www.hellodialog.com",
            "role": "Maintainer"
        },
        {
            "name": "Dave Hoeks",
            "email": "dave@hellodialog.com",
            "homepage": "https://www.hellodialog.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "5.4.36",
        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/promises": "1.3.1",
        "guzzlehttp/psr7": "1.6.1",
        "myclabs/php-enum": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hellodialog\\ApiWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hellodialog\\ApiWrapper\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
